myPDF(theName, 6, 4, mar=c(3.9, 4.9, 1, 1), call=2)#
xlab <- "Lag of the pollutant relative to the departure"#
ylab <- paste("Estimated increase in log(pollutant),",#
				"\nstandardized by the standard error", sep="")#
ylim <- range(coSt)#
plot(LAGS[poll==outcome[1]], rev(coSt[poll==outcome[1]]),#
	 xlab=xlab, ylab=ylab, ylim=ylim, type="l",#
	 col=COL[1], lwd=lwd[1])#
