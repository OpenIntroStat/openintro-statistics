lags    <- -10:10#
nLags   <- length(lags)#
#=====> Processing Data <=====##
coSt <- c()#
se   <- c()#
sem  <- c()#
poll <- c()#
LAGS <- c()#
for(i in 1:length(outcome)){#
	se   <- append(se, SE[[outcome[i]]])#
	sem  <- append(sem, mean(SE[[outcome[i]]]))#
	coSt <- append(coSt, COEF[[outcome[i]]]/sem[i])#
	poll <- append(poll, rep(outcome[i], nLags))#
	LAGS <- append(LAGS, lags)#
}#
#=====> Model summary <=====##
COL     <- c('#00000077', '#0055AAFF', '#5500AA55', '#55AA0099', '#AA5500AA')#
lwd     <- seq(0.7, 2.1, length.out=5)#
