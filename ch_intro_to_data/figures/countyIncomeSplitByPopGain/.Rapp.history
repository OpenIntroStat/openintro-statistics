  	  lines(rep(add,2), c(FENCE[2], FN[4]), lwd=lwd, col=lcol)#
  	  lines(add+w*c(-1,1), rep(FENCE[2], 2), lwd=lwd, col=lcol)#
  	  if(any(show)){#
  	    points(rep(add, sum(show)), x[show],#
  	           pch=pch, cex=pchCex, col=col)#
  	  }#
  	}#
