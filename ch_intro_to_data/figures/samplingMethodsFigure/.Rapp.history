box()#
X    <- c(0.18, 0.3, 0.68, 1.18, 1.4, 1.74)#
Y    <- c(0.2, 0.78, 0.44, 0.7, 0.25, 0.65)#
locs <- c(1, 4, 5, 3, 6, 2)#
gps  <- list()#
N    <- c(15, 12, 35, 22, 13, 28)#
R    <- sqrt(N/500)#
p    <- matrix(c(12, 2, NA,#
				 1,  2, NA,#
				 4,  30, NA,#
				 19, 1, NA,#
				 11, 0, NA,#
				 3, 24, NA), 3)#
p[3,] <- N - p[1,] - p[2,]#
above <- c(1, 1, 1, 1, -1, 1)#
for(i in 1:6){#
	hold <- seq(0, 2*pi, len=99)#
	x    <- X[i] + (R[i]+0.01)*cos(hold)#
	y    <- Y[i] + (R[i]+0.01)*sin(hold)#
	polygon(x, y, border="#CCCCCC")#
	x    <- rep(NA, N[i])#
	y    <- rep(NA, N[i])#
	for(j in 1:N[i]){#
		inside <- FALSE#
		while(!inside){#
			xx <- runif(1, -R[i], R[i])#
			yy <- runif(1, -R[i], R[i])#
			if(sqrt(xx^2 + yy^2) < R[i]){#
				inside <- TRUE#
				x[j]   <- xx#
				y[j]   <- yy#
			}#
		}#
	}#
